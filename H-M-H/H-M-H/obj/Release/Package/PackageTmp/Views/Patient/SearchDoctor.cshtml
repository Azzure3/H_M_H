    <section id="blogArchive">
        <div class="row">
            <div class="col-lg-12 col-md-12">
                <div class="blog-breadcrumbs-area">
                    <div class="container">
                        <div class="blog-breadcrumbs-left">
                            <h2>Search Doctor</h2>
                        </div>
                        <div class="blog-breadcrumbs-right">
                            <ol class="breadcrumb">
                                <li>You are here</li>
                                <li><a href="/Patient/Index">Patient</a></li>
                                <li class="active">Search Doctor</li>
                            </ol>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
  <br><br><br><br><br><br>
        <div class="col-md-10">
            <!-- Advanced Tables -->
            <div class="panel panel-default">
                <div style="font-size:medium;" class="panel-heading btn-primary">
                    <b>View Doctors</b>
                </div>

               
               

                <div class="panel-body" style="font-size:medium">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover" id="Inventroy-datatable">
                            <thead style="text-align:center">
                                <tr>
                                    <th>Sr. No</th>
                                    <th>First Name</th>
                                    <th>Last Name</th>
                                    <th>User Name</th>
                                    <th>Gender</th>
                                    <th>Specification</th>
                                    <th>Email</th>
                                    <th>Phone No</th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                @if (doc_list != null)
                                {
                                    for (int i = 0; i < doc_list.Count; i++)
                                    {
                                        <tr class="odd gradeX">
                                            <td>@(i + 1)</td>
                                            <td>@Html.DisplayFor(m => doc_list[i].User.f_Name)</td>
                                            <td>@Html.DisplayFor(m => doc_list[i].User.l_Name)</td>
                                            <td>@Html.DisplayFor(m => doc_list[i].User.userName)</td>
                                            <td>@Html.DisplayFor(m => doc_list[i].User.Gender)</td>
                                            <td>@Html.DisplayFor(m => doc_list[i].Specification)</td>
                                            <td>@Html.DisplayFor(m => doc_list[i].User.Email)</td>
                                            <td>@Html.DisplayFor(m => doc_list[i].User.PhoneNo)</td>
                                            <td style="text-align:center">
                                                <a href="@Url.Action("Details", "Inventory", new { id = doc_list[i].Id })" class="btn btn-info">Appointment</a>
                                            </td>
                                        </tr>
                                    }
                                }

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <!--End Advanced Tables -->
        </div>
    








@{
    ViewBag.Title = "View_Patient";
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ViewPatients</title>
    <!-- Bootstrap css file-->
    <link href="~/css/bootstrap.min.css" rel="stylesheet">
    <!-- Font awesome css file-->
    <link href="~/css/font-awesome.min.css" rel="stylesheet">
    <!-- Default Theme css file -->
    <link id="switcher" href="~/css/themes/default-theme.css" rel="stylesheet">
    <!-- Slick slider css file -->
    <link href="~/css/slick.css" rel="stylesheet">
    <!-- Photo Swipe Image Gallery -->
    <link rel='stylesheet prefetch' href="~/css/photoswipe.css">
    <link rel='stylesheet prefetch' href="~/css/default-skin.css">

    <!-- Main structure css file -->
    <link href="~/style.css" rel="stylesheet">
</head>
<body style="font-size:large">

    <section id="blogArchive">
        <div class="row">
            <div class="col-lg-12 col-md-12">
                <div class="blog-breadcrumbs-area">
                    <div class="container">
                        <div class="blog-breadcrumbs-left">
                            <h2>View Ptient</h2>
                        </div>
                        <div class="blog-breadcrumbs-right">
                            <ol class="breadcrumb">
                                <li>You are here</li>
                                <li><a href="/Doctor/Index">Patient</a></li>
                                <li class="active">View Patient</li>
                            </ol>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <br><br><br><br><br><br>
    <header><h1 style="font-family:'Felix Titling'"> View Patients </h1></header>
    <div class="col-lg-11">
        <br>
        <table class="table table-bordered" style="border:solid; margin-top:15px;">
            <thead>
                <tr class="btn-primary" style="text-align:center ; font-size:20px">
                    <th>Id</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>User Name</th>
                    <th>Gender</th>
                    <th>Age</th>
                    <th>Contact No</th>
                    <th>Email</th>
                    <th>Address</th>


                </tr>
            </thead>

            @foreach (H_M_H.Models.User obj in Model) 
            {
                <tr style = "font-size:15px">
                    <td style="font-size:15px">@obj.Id</td>
                    <td style="font-size:15px">@obj.f_Name</td>
                    <td style="font-size:15px">@obj.l_Name</td>
                    <td style="font-size:15px">@obj.userName</td>
                    <td style="font-size:15px">@obj.Gender</td>
                    <td style="font-size:15px">@obj.Age</td>
                    <td style="font-size:15px">@obj.PhoneNo</td>
                    <td style="font-size:15px">@obj.Email</td>
                    <td style="font-size:15px">@obj.Address</td>
         

                </tr>
            }

        </table>
    </div>


</body>
</html>
